  <Color x:Key="TileRegionColor">#262626</Color>
  <Color x:Key="TileBorderColor">#14000000</Color>
  <Color x:Key="AcrylicTrimForeground">#FCFCFC</Color>
  <Color x:Key="AcrylicTrimBackground">#90134074</Color>

